<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginCl</servlet-name>
    <servlet-class>com.ctt.servlet.LoginCl</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowCartCl</servlet-name>
    <servlet-class>com.ctt.servlet.ShowCartCl</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Logintiaozhuan</servlet-name>
    <servlet-class>com.ctt.servlet.Logintiaozhuan</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>LoginCl</servlet-name>
    <url-pattern>/LoginCl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowCartCl</servlet-name>
    <url-pattern>/ShowCartCl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logintiaozhuan</servlet-name>
    <url-pattern>/Logintiaozhuan</url-pattern>
  </servlet-mapping>

</web-app>